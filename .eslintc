{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "no-console": 1
  },
  "plugins": [
    "import",
    "jsx-a11y"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "16.5.2"
    }
  }
}
